{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "typicals to process with yacg",
    "description": "description of the types for yacg",
    "version": "0.0.1",
    "x-domain": "yacg.model.config",
    "definitions": {
      "AQType": {
        "type": "object",
        "description": "Anlagenr√ºckstellung",
        "properties": {
            "__isObjectType": {
                "description": "tells if this definition is an object type",
                "type": "boolean",
                "default": true
            },
            "I_EventResetMode": {
                "description": "the member I_EventResetMode",
                "type": "object",
                "properties": {
                    "__isDataProperty": {
                        "description": "tells if this is definition a data property",
                        "type": "boolean",
                        "default": true
                    },
                    "__dataType": {
                        "description": "Assigns a data type to this data variable",
                        "$ref": "#/definitions/EventState"
                    },
                    "prio": {
                        "description": "prio number",
                        "type": "integer",
                        "default": 1
                    },
                    "internalType": {
                        "description": "internal type (event, measurement, command)",
                        "type": "integer",
                        "default": 0
                    },
                    "historizing": {
                        "description": "historizing flag (on, off)",
                        "type": "boolean",
                        "default": true
                    }
                },
                "required": [
                    "__isDataProperty",
                    "__dataType",
                    "prio",
                    "internalType",
                    "historizing"
                ]
            },
            "S_SetResetEventMode": {
                "description": "the member S_SetResetEventMode",
                "type": "object",
                "properties": {
                    "__isMethod": {
                        "description": "tells if this definition is a method",
                        "type": "boolean",
                        "default": true
                    },
                    "__arguments": {
                        "description": "method argument",
                        "$ref": "#/definitions/EventState"
                    },
                    "prio": {
                        "description": "prio number",
                        "type": "integer",
                        "default": 1
                    },
                    "historizing": {
                        "description": "historizing flag (on, off)",
                        "type": "boolean",
                        "default": false
                    }
                },
                "required": [
                    "__isMethod",
                    "__arguments",
                    "prio",
                    "historizing"
                ]
            }
        },
        "required": [
            "__isObjectType",
            "I_EventResetMode",
            "S_SetResetEventMode"
        ]
      },
      "EventState": {
          "type": "object",
          "description": "Data type for I_EventResetMode, but could also be for other attributes",
          "properties": {
            "__isDataType": {
                "description": "tells if this definition is a data type",
                "type": "boolean",
                "default": true
            },
            "format": {
                "type": "integer",
                "enum": [0, 1]
            }
        }
      }
    }
  }
  